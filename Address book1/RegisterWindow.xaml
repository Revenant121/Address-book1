<Window x:Class="Address_book1.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Регистрация" Height="300" Width="300">
	<Grid>
		<StackPanel Margin="20">
			<TextBox Name="UsernameBox" Margin="0,0,0,10" Text="Имя пользователя"/>
			<PasswordBox Name="PasswordBox" Margin="0,0,0,10"/>
			<PasswordBox Name="ConfirmPasswordBox" Margin="0,0,0,10"/>
			<TextBox  Name="EmailBox"          Text="Email" />
			<Button Content="Зарегистрироваться" Click="Register_Click"/>
		
		</StackPanel>
	</Grid>
</Window>
